{
  "name": "viewer-devtools",
  "displayName": "Viewer devtools",
  "version": "2.0.1",
  "description": "A basic Plasmo extension.",
  "author": "nirn",
  "scripts": {
    "dev": "plasmo dev",
    "build": "plasmo build",
    "deploy": "yarn version patch && git add . && yarn build && zip -r build/prod.zip build/chrome-mv3-prod && yarn open-dashboard && open build",
    "open-dashboard": "open 'https://chrome.google.com/webstore/devconsole/668cb2ff-2b78-49a3-a7bd-4257b2545885/aahcojdfiikihlpfamimldmionifllkl/edit/package?hl=en_US'"
  },
  "dependencies": {
    "@babel/core": "^7.0.0",
    "@emotion/react": "^11.9.0",
    "@emotion/styled": "^11.8.1",
    "@mui/material": "^5.8.3",
    "octokit": "^2.0.4",
    "react": "18.1.0",
    "react-dom": "18.1.0"
  },
  "devDependencies": {
    "@parcel/core": "^2.6.2",
    "@parcel/transformer-sass": "^2.6.2",
    "@plasmohq/storage": "^0.8.1",
    "@trivago/prettier-plugin-sort-imports": "3.3.0",
    "@types/chrome": "0.0.193",
    "@types/gtag.js": "^0.0.10",
    "@types/node": "18.6.1",
    "@types/react": "18.0.15",
    "@types/react-dom": "18.0.6",
    "plasmo": "latest",
    "prettier": "2.7.1",
    "typescript": "4.7.4"
  },
  "manifest": {
    "web_accessible_resources": [
      {
        "resources": [
          "~assets/*.png"
        ],
        "matches": [
          "https://*/*"
        ]
      }
    ],
    "host_permissions": [
      "http://*/*",
      "https://*/*"
    ],
    "permissions": [
      "http://*/*",
      "https://*/*",
      "proxy",
      "storage",
      "scripting",
      "action",
      "declarativeNetRequest",
      "tabs"
    ]
  },
  "packageManager": "yarn@3.2.2"
}
